    ///////////////////////////////////////////////////////////////////////////////////////
    //
    // Data properties
    //
    ///////////////////////////////////////////////////////////////////////////////////////
     -->

    


    <!-- http://purl.org/dc/terms/subject -->

    <owl:DatatypeProperty rdf:about="&dct;subject"/>
    


    <!-- http://purl.org/sport/ontology/characteristic -->

    <owl:DatatypeProperty rdf:about="&sport;characteristic">
        <rdfs:label xml:lang="de">ausprägung</rdfs:label>
        <rdfs:label xml:lang="en">characteristic</rdfs:label>
        <rdfs:comment xml:lang="de">Die Art des Matches oder ein anderes Merkmal.</rdfs:comment>
        <rdfs:comment xml:lang="en">The type of match or other feature.</rdfs:comment>
        <rdfs:subPropertyOf rdf:resource="&dct;subject"/>
        <rdfs:domain rdf:resource="&sport;Match"/>
        <rdfs:subPropertyOf rdf:resource="&xsd;label"/>
        <rdfs:range rdf:resource="&xsd;string"/>
    </owl:DatatypeProperty>
    


    <!-- http://purl.org/sport/ontology/city -->

    <owl:DatatypeProperty rdf:about="&sport;city">
        <rdfs:domain rdf:resource="&sport;Venue"/>
        <rdfs:range rdf:resource="&xsd;string"/>
    </owl:DatatypeProperty>
    


    <!-- http://purl.org/sport/ontology/class -->

    <owl:DatatypeProperty rdf:about="&sport;class">
        <rdfs:label xml:lang="en">class </rdfs:label>
        <rdfs:domain rdf:resource="&sport;Tournament"/>
        <rdfs:range rdf:resource="&rdfs;Literal"/>
    </owl:DatatypeProperty>
    


    <!-- http://purl.org/sport/ontology/classLevel -->

    <owl:DatatypeProperty rdf:about="&sport;classLevel">
        <rdfs:label xml:lang="en">league system level</rdfs:label>
        <rdfs:comment xml:lang="en">Arrange tournaments hierarchically forming a pyramid. The higher the value, the weaker the performance level.</rdfs:comment>
        <rdfs:domain rdf:resource="&sport;Tournament"/>
        <rdfs:range rdf:resource="&xsd;int"/>
    </owl:DatatypeProperty>
    


    <!-- http://purl.org/sport/ontology/division -->

    <owl:DatatypeProperty rdf:about="&sport;division">
        <rdfs:label xml:lang="en">division </rdfs:label>
        <rdfs:comment xml:lang="en">Divisions allow leagues to be split into smaller groups, e.g. based on region.</rdfs:comment>
        <rdfs:domain rdf:resource="&sport;League"/>
        <rdfs:range rdf:resource="&rdfs;Literal"/>
    </owl:DatatypeProperty>
    


    <!-- http://purl.org/sport/ontology/homeAdvantage -->

    <owl:DatatypeProperty rdf:about="&sport;homeAdvantage">
        <rdfs:label xml:lang="en">home advantage</rdfs:label>
        <rdfs:domain rdf:resource="&sport;TeamScore"/>
        <rdfs:range rdf:resource="&xsd;boolean"/>
    </owl:DatatypeProperty>
    


    <!-- http://purl.org/sport/ontology/originalScore -->

    <owl:DatatypeProperty rdf:about="&sport;originalScore">
        <rdfs:label xml:lang="en">original score</rdfs:label>
        <rdfs:comment xml:lang="en">The original score in case the final score had to be changed due to formal reasons.</rdfs:comment>
        <rdfs:domain rdf:resource="&sport;TeamScore"/>
        <rdfs:range rdf:resource="&xsd;int"/>
    </owl:DatatypeProperty>
    


    <!-- http://purl.org/sport/ontology/postalCode -->

    <owl:DatatypeProperty rdf:about="&sport;postalCode">
        <rdfs:domain rdf:resource="&sport;Venue"/>
        <rdfs:range rdf:resource="&xsd;string"/>
    </owl:DatatypeProperty>
    


    <!-- http://purl.org/sport/ontology/promoted -->

    <owl:DatatypeProperty rdf:about="&sport;promoted">
        <rdfs:label xml:lang="en">promoted </rdfs:label>
        <rdfs:domain rdf:resource="&sport;League"/>
        <rdfs:range rdf:resource="&rdfs;Literal"/>
    </owl:DatatypeProperty>
    


    <!-- http://purl.org/sport/ontology/rank -->

    <owl:DatatypeProperty rdf:about="&sport;rank">
        <rdfs:label xml:lang="en">rank</rdfs:label>
        <rdfs:domain rdf:resource="&sport;TeamScore"/>
        <rdfs:range rdf:resource="&xsd;nonNegativeInteger"/>
    </owl:DatatypeProperty>
    


    <!-- http://purl.org/sport/ontology/relegated -->

    <owl:DatatypeProperty rdf:about="&sport;relegated">
        <rdfs:label xml:lang="en">relegated </rdfs:label>
        <rdfs:domain rdf:resource="&sport;League"/>
        <rdfs:range rdf:resource="&rdfs;Literal"/>
    </owl:DatatypeProperty>
    


    <!-- http://purl.org/sport/ontology/score -->

    <owl:DatatypeProperty rdf:about="&sport;score">
        <rdfs:label xml:lang="en">score</rdfs:label>
        <rdfs:comment xml:lang="en">The decisive measurement for the teams success in a match. Can be the goals in football or the time in a swimming competition.</rdfs:comment>
        <rdfs:domain rdf:resource="&sport;TeamScore"/>
        <rdfs:range rdf:resource="&xsd;int"/>
    </owl:DatatypeProperty>
    


    <!-- http://purl.org/sport/ontology/season -->

    <owl:DatatypeProperty rdf:about="&sport;season">
        <rdfs:label xml:lang="en">season</rdfs:label>
        <rdfs:domain rdf:resource="&sport;Tournament"/>
        <rdfs:range rdf:resource="&xsd;string"/>
    </owl:DatatypeProperty>
    


    <!-- http://purl.org/sport/ontology/street -->

    <owl:DatatypeProperty rdf:about="&sport;street">
        <rdfs:domain rdf:resource="&sport;Venue"/>
        <rdfs:range rdf:resource="&xsd;string"/>
    </owl:DatatypeProperty>
    


    <!-- http://purl.org/sport/ontology/teamNumber -->

    <owl:DatatypeProperty rdf:about="&sport;teamNumber">
        <owl:maxCardinality rdf:datatype="&xsd;nonNegativeInteger">1</owl:maxCardinality>
        <rdfs:domain rdf:resource="&sport;Team"/>
        <rdfs:range rdf:resource="&xsd;int"/>
    </owl:DatatypeProperty>
    


    <!-- http://www.w3.org/2001/XMLSchema#label -->

    <owl:DatatypeProperty rdf:about="&xsd;label"/>
    


