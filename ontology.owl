    </owl:AnnotationProperty>
    


    <!-- http://www.w3.org/2002/07/owl#maxCardinality -->

    <owl:AnnotationProperty rdf:about="&owl;maxCardinality"/>
    


    <!-- http://xmlns.com/foaf/0.1/homepage -->

    <owl:AnnotationProperty rdf:about="&foaf;homepage"/>
    


    <!-- http://xmlns.com/foaf/0.1/name -->

    <owl:AnnotationProperty rdf:about="&foaf;name"/>
    


    <!-- 
    ///////////////////////////////////////////////////////////////////////////////////////
    //
    // Object Properties
    //
    ///////////////////////////////////////////////////////////////////////////////////////
     -->

    


    <!-- http://purl.org/NET/c4dm/event.owl#sub_event -->

    <owl:ObjectProperty rdf:about="http://purl.org/NET/c4dm/event.owl#sub_event"/>
    


    <!-- http://purl.org/dc/terms/hasPart -->

    <owl:ObjectProperty rdf:about="&dct;hasPart"/>
    


    <!-- http://purl.org/sport/ontology/associatedSportsClub -->

    <owl:ObjectProperty rdf:about="&sport;associatedSportsClub">
        <rdf:type rdf:resource="&owl;FunctionalProperty"/>
        <rdfs:range rdf:resource="&sport;SportsClub"/>
        <rdfs:domain rdf:resource="&sport;Team"/>
        <owl:inverseOf rdf:resource="&sport;fieldsTeam"/>
    </owl:ObjectProperty>
    


    <!-- http://purl.org/sport/ontology/captain -->

    <owl:ObjectProperty rdf:about="&sport;captain">
        <rdfs:label xml:lang="de">mannschaftsleiter</rdfs:label>
        <rdfs:label xml:lang="en">captain</rdfs:label>
        <rdfs:domain rdf:resource="&sport;Team"/>
        <rdfs:range rdf:resource="&foaf;Person"/>
    </owl:ObjectProperty>
    


    <!-- http://purl.org/sport/ontology/coach -->

    <owl:ObjectProperty rdf:about="&sport;coach">
        <rdfs:label xml:lang="de">trainer</rdfs:label>
        <rdfs:label xml:lang="en">coach</rdfs:label>
        <rdfs:domain rdf:resource="&sport;Team"/>
        <rdfs:range rdf:resource="&foaf;Person"/>
    </owl:ObjectProperty>
    


    <!-- http://purl.org/sport/ontology/fieldsTeam -->

    <owl:ObjectProperty rdf:about="&sport;fieldsTeam">
        <rdfs:label xml:lang="de">stellt Team auf</rdfs:label>
        <rdfs:label xml:lang="en">fields Team</rdfs:label>
        <rdfs:comment xml:lang="de">Ein Sportverein kann viele Mannschaften aufstellen.</rdfs:comment>
        <rdfs:comment xml:lang="en">A Sports Club can field many Teams.</rdfs:comment>
        <rdfs:domain rdf:resource="&sport;SportsClub"/>
        <rdfs:range rdf:resource="&sport;Team"/>
    </owl:ObjectProperty>
    


    <!-- http://purl.org/sport/ontology/organizedBy -->

    <owl:ObjectProperty rdf:about="&sport;organizedBy">
        <rdfs:label xml:lang="en">organized by</rdfs:label>
        <rdfs:range rdf:resource="&sport;SportOrganization"/>
        <rdfs:domain rdf:resource="&sport;SportsEvent"/>
    </owl:ObjectProperty>
    


    <!-- http://purl.org/sport/ontology/outcome -->

    <owl:ObjectProperty rdf:about="&sport;outcome">
        <rdfs:domain rdf:resource="&sport;SportsEvent"/>
        <rdfs:range rdf:resource="&sport;TeamScore"/>
    </owl:ObjectProperty>
    


    <!-- http://purl.org/sport/ontology/partOfEvent -->

    <owl:ObjectProperty rdf:about="&sport;partOfEvent">
        <rdf:type rdf:resource="&owl;FunctionalProperty"/>
        <rdfs:label xml:lang="en">part of Event</rdfs:label>
        <rdfs:range rdf:resource="&sport;SportsEvent"/>
        <owl:inverseOf rdf:resource="&sport;subEvent"/>
    </owl:ObjectProperty>
    


    <!-- http://purl.org/sport/ontology/participant -->

    <owl:ObjectProperty rdf:about="&sport;participant">
        <rdfs:range rdf:resource="&sport;Athlete"/>
        <rdfs:range rdf:resource="&sport;Team"/>
        <rdfs:domain rdf:resource="&sport;TeamScore"/>
    </owl:ObjectProperty>
    


    <!-- http://purl.org/sport/ontology/place -->

    <owl:ObjectProperty rdf:about="&sport;place">
        <rdfs:range rdf:resource="&sport;Venue"/>
        <rdfs:domain rdf:resource="&owl;Thing"/>
    </owl:ObjectProperty>
    


    <!-- http://purl.org/sport/ontology/play -->

    <owl:ObjectProperty rdf:about="&sport;play">
        <rdfs:range rdf:resource="&sport;FOCUSport_Activity"/>
    </owl:ObjectProperty>
    


    <!-- http://purl.org/sport/ontology/qualifiesFor -->

    <owl:ObjectProperty rdf:about="&sport;qualifiesFor">
        <rdfs:label xml:lang="en">qualifies for</rdfs:label>
        <rdfs:comment xml:lang="en">Multi-stage tournaments can have &quot;regular season&quot; (e.g. group tournament) acting as qualification for the &quot;post season&quot; or &quot;playoffs&quot; (e.g. single-elimination tournament)</rdfs:comment>
        <rdfs:range rdf:resource="&sport;Tournament"/>
        <rdfs:domain rdf:resource="&sport;Tournament"/>
    </owl:ObjectProperty>
    


    <!-- http://purl.org/sport/ontology/rankingMethod -->

    <owl:ObjectProperty rdf:about="&sport;rankingMethod">
        <rdfs:label xml:lang="en">ranking method</rdfs:label>
        <rdfs:comment xml:lang="en">Determines the tournament type.</rdfs:comment>
        <rdfs:range rdf:resource="&sport;RankingSystem"/>
        <rdfs:domain rdf:resource="&sport;Tournament"/>
    </owl:ObjectProperty>
    


    <!-- http://purl.org/sport/ontology/subEvent -->

    <owl:ObjectProperty rdf:about="&sport;subEvent">
        <rdfs:label xml:lang="de">unter-Veranstaltung</rdfs:label>
        <rdfs:label xml:lang="en">sub-Event</rdfs:label>
        <rdfs:comment xml:lang="de">Eine Veranstaltung kann mehrere andere Turniere oder Matches umfassen, z.B. Sätze und Spiele. Objekt ist ein SportsEvent oder zum Beispiel eine Folge (rdf:Seq) von Matches, wenn deren Spiel-Reihenfolge wichtig ist.</rdfs:comment>
        <rdfs:comment xml:lang="en">A sports event comprises other tournaments or single matches, like sets or games. Object is a SportsEvent or for example a sequence (rdf:Seq) of Matches when the playing order is important.</rdfs:comment>
        <rdfs:subPropertyOf rdf:resource="http://purl.org/NET/c4dm/event.owl#sub_event"/>
        <rdfs:subPropertyOf rdf:resource="&dct;hasPart"/>
        <rdfs:domain rdf:resource="&sport;SportsEvent"/>
    </owl:ObjectProperty>
    


    <!-- http://purl.org/sport/ontology/teamMate -->

    <owl:ObjectProperty rdf:about="&sport;teamMate">
        <rdfs:range rdf:resource="&sport;Athlete"/>
        <rdfs:domain rdf:resource="&sport;Team"/>
        <rdfs:subPropertyOf rdf:resource="&foaf;member"/>
    </owl:ObjectProperty>
    


    <!-- http://xmlns.com/foaf/0.1/member -->

    <owl:ObjectProperty rdf:about="&foaf;member"/>
    


